{
  "name": "fire-jira-app",
  "version": "1.0.0",
  "description": "Jira clone wrote in React.js and Firebase",
  "main": "index.js",
  "scripts": {
    "start": "yarn clean && parcel public/index.html -p 8080",
    "build": "yarn lint && yarn test && parcel build public/index.html --no-cache --out-dir dist --public-url ./",
    "deploy": "firebase deploy",
    "clean": "rimraf -rf ./.cache",
    "lint": "eslint --fix --ext js,jsx,ts,tsx src",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/witoldmetel/Fire-Jira-App"
  },
  "keywords": [
    "parcel",
    "typescript",
    "react",
    "sass",
    "firebase"
  ],
  "author": "Witold Metel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/witoldmetel/Fire-Jira-App/issues"
  },
  "homepage": "fire-jira.firebaseapp.com",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/runtime-corejs3": "^7.9.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.2",
    "@types/p5": "0.9.0",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "babel-jest": "25.5.1",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "firebase": "^7.14.4",
    "formik": "^2.1.5",
    "jest": "25.5.4",
    "p5": "1.0.0",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "react-is": "^16.13.1",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^3.4.0",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "redux": "^3.7.2",
